{
  "trailingComma": "none",
  "bracketSpacing": false,
  "bracketSameLine": false,
  "singleAttributePerLine": false,
  "printWidth": 160,
  "overrides": [
    {
      "files": "**/lwc/**/*.html",
      "options": {
        "parser": "lwc"
      }
    },
    {
      "files": "*.{cmp,page,component}",
      "options": {"parser": "html"}
    }
  ]
}
